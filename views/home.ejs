<%- include("partials/header"); -%>

    <h1>Home</h1>

    <p> <%= startingContent %> </p>

    <% posts.forEach(function(post) { %>

      <h1><%= post.title.substring(0,20) + "..." %></h1>

      <p>
        <%= post.content.substring(0,120) + "..." %>
        <a href="posts/<%= post.title %>">Read More...</a>
      </p>

    <% }); %>

<%- include("partials/footer"); -%>
